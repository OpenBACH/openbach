{
  "name": "Pre-Recette",
  "description": "Pre-Recette 07/11/2016",
  "entity": [
    {
      "name": "Sat",
      "description": "The satellite",
      "agent": {
          "name": "p2-sat",
          "address": "172.20.34.26",
          "username": "opensand",
          "collector": "172.20.34.37"
      },
      "networks": [
        "emu"
      ]
    },
    {
      "name": "gw0",
      "description": "Gateway",
      "agent": {
          "name": "p2-gw0",
          "address": "172.20.34.38",
          "username": "opensand",
          "collector": "172.20.34.37"
      },
      "networks": [
        "emu",
        "lan_gw"
      ]
    },
    {
      "name": "st1",
      "description": "Satellite terminal 1",
      "agent":  {
          "name": "p2-st1",
          "address": "172.20.34.30",
          "username": "opensand",
          "collector": "172.20.34.37"
    },
      "networks": [
        "emu",
        "lan_st"
      ]
    }
  ],
  "network": [
    "emu",
    "lan_gw",
    "lan_st"
  ],
  "scenario": [
    {
      "name": "Ping",
      "description": "Comparaison between 2 ping Jobs",
      "arguments": {},
      "constants": {
          "gw0": "172.20.34.38",
          "st1": "172.20.34.30"
      },
      "openbach_functions": [
        {
          "id": 1,
          "start_job_instance": {
            "agent_ip": "$gw0",
            "iperf": {
              "mode": "-s"
            },
            "offset": 0
          }
        },
        {
          "id": 2,
          "start_job_instance": {
            "agent_ip": "$st1",
            "iperf": {
              "mode": "-c 172.20.34.38"
            },
            "offset": 0
          },
          "wait": {
            "launched_ids": [
              1
            ]
          }
        },
        {
          "id": 3,
          "start_job_instance": {
            "agent_ip": "$st1",
            "ping": {
              "count": "3",
              "destination_ip": "$gw0"
            },
            "offset": 0
          }
        },
        {
          "id": 4,
          "start_job_instance": {
            "agent_ip": "$st1",
            "ping": {
              "destination_ip": "$gw0"
            },
            "offset": 0
          }
        }
      ]
    },
    {
      "name": "Sandbox",
      "description": "Sandbox",
      "arguments": {},
      "constants": {},
      "openbach_functions": []
    }
  ]
}
